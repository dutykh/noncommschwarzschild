<Group view="code" labelreference="L2" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># The Extreme case:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L3" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">restart: # Matrices assembling for non-commutative Schwarchild black-holes' quasi-normal modes computation</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L4" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">with(LinearAlgebra):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L5" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Digits := 200: # Number of significant digits we compute</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L6" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># Physical parameters:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L8" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">s := 0: # spin parameter value (s = 0, 1, 2)</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L7" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">e := 1 - s^2: # self-explaining</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L9" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">L := 0: # angular momentum parameter value</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L10" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># Computation of the dimensionless mass and event horizon parameters:</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L11" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">f := (xe, mu) -&gt; 1 - 1/xe*erf(mu*xe) + 2*mu/sqrt(Pi)*exp(-mu^2*xe^2):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L108" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">sol := fsolve({f(xe,mu) = 0, diff(f(xe,mu),xe) = 0},{xe = 0..3, mu = 0..3}):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L12" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">assign(sol); # we assign the values of critical parameters</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L14" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># Numerical parameters:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L15" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">n := 150: # Number of Tchebyshev modes, the matrices will be of size n*n</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L16" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">a := 1 + (2*xe*(mu^4*xe^4 - mu^2*xe^2 - 1))/(3*(mu^2*xe^2 - 1)^2):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L17" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">f := y -&gt; 1 - 1/2*(1 - y)*erf(2*mu*xe/(1 - y))/xe + 2*mu*exp(-4*mu^2*xe^2/(1 - y)^2)/sqrt(Pi):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L59" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">fp := y -&gt; erf(2*mu*xe/(1 - y))/(2*xe) - 2*exp(-4*mu^2*xe^2/(1 - y)^2)*mu/((1 - y)*sqrt(Pi)) - 16*mu^3*xe^2*exp(-4*mu^2*xe^2/(1 - y)^2)/((1 - y)^3*sqrt(Pi)):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L18" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">V := y -&gt; (1 - y)^2/16*f(y)*(e*(1 - y)*fp(y) + L*(L + 1)):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L111" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">eta := y -&gt; (1 + y)/(1 - y) + (1 - y)/((mu^2*xe^2 - 1)*(1 + y)):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L112" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">etap := y -&gt; ((2*mu^2*xe^2 - 4)*y^2 + 4*mu^2*xe^2*y + 2*mu^2*xe^2 - 4)/((-1 + y)^2*(mu^2*xe^2 - 1)*(1 + y)^2):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L113" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">etapp := y -&gt; ((-4*mu^2*xe^2 + 8)*y^3 - 12*mu^2*xe^2*y^2 + (-12*mu^2*xe^2 + 24)*y - 4*mu^2*xe^2)/((-1 + y)^3*(mu^2*xe^2 - 1)*(1 + y)^3):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L22" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># Definition of non-zero boundary limits:</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L20" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">A0e := -1/16*(L + 1)*L:</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L25" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">B0e := 1/16*(mu^2*xe^2 - 1)*(L + 1)*L:</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L28" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Lam1e := -xe/2*(mu^2*xe^2 - 1):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L29" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">A2e := (2*mu^4*xe^6 + 3*mu^4*xe^4 + mu^2*xe^4 - 6*mu^2*xe^2 - 5*xe^2 + 3)/(6*(mu^2*xe^2 - 1)^2):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L30" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">B2e := xe*(2*mu^8*xe^9 + 35*mu^6*xe^7 + 18*mu^6*xe^6 - 107*mu^4*xe^5 - 54*mu^4*xe^4 + 88*mu^2*xe^3 + 54*mu^2*xe^2 - 10*xe - 18)/(36*(mu^2*xe^2 - 1)^2):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L19" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># Definition of final ODE coefficients:</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L21" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">L00 := y -&gt; piecewise(y=-1, B0e, y=1, A0e, (y &gt; -1) and (y &lt; 1), -4*V(y)/(-y^2 + 1)^2):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L31" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">L01 := y -&gt; piecewise(y=-1, 0, y=1, 0, (y &gt; -1) and (y &lt; 1), 1/4*f(y)/(1+y)^2*(-2*(1-y)*f(y) + (1-y)^2*fp(y))):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L32" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">L02 := y -&gt; piecewise(y=-1, 0, y=1, 0, (y &gt; -1) and (y &lt; 1), (1 - y)^2/(4*(1 + y)^2)*f(y)^2):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L33" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">L10 := y -&gt; piecewise(y=-1, 0, y=1, 0, (y &gt; -1) and (y &lt; 1), xe/2*(1 - y)^2*f(y)*fp(y)*etap(y)/(1 + y)^2 + xe/2*(1 - y)*f(y)^2*((1 - y)*etapp(y) - 2*etap(y))/(1 + y)^2 + 1/2*(1 - y)*(2 - a*(1 - y))*f(y)*fp(y)/(1 + y)^3 - f(y)^2*(2 - 2*a*(1 - y) + 1/2*a*(1 - y)^2)/(1 + y)^4):</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L34" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">L11 := y -&gt; piecewise(y=-1, Lam1e, y=1, xe/2, (y &gt; -1) and (y &lt; 1), (1 - y)*f(y)^2*(xe*(-y^2 + 1)*etap(y) + 2 - a*(1 - y))/(1 + y)^3):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L35" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">L12 := y -&gt; 0:</Text-field>
</Input>
</Group>
<Group view="code" hide-input="false" labelreference="L36" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">L20 := y -&gt; piecewise(y=-1, B2e, y=1, A2e, (y &gt; -1) and (y &lt; 1), 4*xe^2/(-y^2 + 1)^2 - f(y)^2*(xe*(-y^2 + 1)*etap(y) + 2 - a*(1 - y))^2/(1 + y)^4):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L37" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">L21 := y -&gt; 0:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L38" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">L22 := y -&gt; 0:</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L23" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">local j: F := y -&gt; simplify(add(a[j]*ChebyshevT(j, y), j=0..n-1)):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L24" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">M0 := Matrix(n): M1 := Matrix(n): M2 := Matrix(n):</Text-field>
</Input>
</Group>
<Group view="code" labelreference="L39" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal"># Assembling the matrices:
for i from 1 to n do
  xi := cos((2.0*i-1.0)*Pi/(2.0*n)); # Chebyshev roots collocation points
  # xi := -cos(Pi*(i-1)/(n-1)); # [-1, 1] Chebyshev extrema collocation points
  expr0 := evalf(L00(xi)*F(xi) + L01(xi)*subs(x=xi, diff(F(x),x)) + L02(xi)*subs(x=xi, diff(F(x),x$2))):
  expr1 := evalf(L10(xi)*F(xi) + L11(xi)*subs(x=xi, diff(F(x),x)) + L12(xi)*subs(x=xi, diff(F(x),x$2))):
  expr2 := evalf(L20(xi)*F(xi) + L21(xi)*subs(x=xi, diff(F(x),x)) + L22(xi)*subs(x=xi, diff(F(x),x$2))):
  for j from 1 to n do
    M0[i,j] := coeff(expr0, a[j-1]);
    M1[i,j] := coeff(expr1, a[j-1]);
    M2[i,j] := coeff(expr2, a[j-1]);
  end do:
end do:</Text-field>
</Input>
</Group>
